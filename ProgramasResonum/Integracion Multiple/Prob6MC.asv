function I = Prob6MC(n)

    F = @(x, y, z) 2 * z * exp(-x^2);
    a = 0; b = 1;
    e = 1; f = 4; 

    xk = a + (b - a) * rand(n, 1)
    yk = 0 + (xk - 0) .* rand(n, 1)
    zk = e + (f - e) * rand(n, 1)

    sum = 0;

    for i = 1:n
        xn = rand();
        yn = rand()

        sum = sum + F(xk(i), yk(i), zk(i));
    end

    I = 1.5 * (1 / n) * sum;
end